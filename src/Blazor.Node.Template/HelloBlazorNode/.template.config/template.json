{
  "$schema": "http://json.schemastore.org/template",
  "author": "mingyaulee",
  "classifications": [
    "Web",
    "Blazor",
    "WebAssembly",
    "Node"
  ],
  "name": "Node application with Blazor WebAssembly",
  "description": "Template to create a Node application with Blazor WebAssembly.",
  "identity": "Blazor.Node",
  "shortName": "blazornode",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "HelloBlazorNode",
  "defaultName": "BlazorNodeApplication1",
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net5.0",
          "description": "Target .NET 5.0"
        },
        {
          "choice": "net6.0",
          "description": "Target .NET 6.0"
        }
      ],
      "defaultValue": "net5.0",
      "replaces": "net5.0"
    }
  }
}